<view class="uni-tab-bar"><view class="head back_3"><view>我的团队</view><view>{{team_count}}</view><view>{{"我的邀请码:"+user.invitecode}}</view></view><scroll-view class="uni-swiper-tab" style="background-color:#E9CEA1;font-weight:bold;" id="tab-bar" scroll-x scroll-left="{{scrollLeft}}"><block wx:for="{{tabBars}}" wx:for-item="tab" wx:for-index="index" wx:key="id"><view class="{{['swiper-tab-list',tabIndex==index?'active':'']}}" id="{{tab.id}}" data-current="{{index}}" data-event-opts="{{[['tap',[['tapTab',['$event']]]]]}}" bindtap="__e">{{tab.name+''}}<view hidden="{{!(tabIndex==index)}}" style="height:4rpx;background-color:#A9731A;width:100%;margin:auto;"></view></view></block></scroll-view><swiper class="swiper-box" current="{{tabIndex}}" duration="{{300}}" data-event-opts="{{[['change',[['changeTab',['$event']]]]]}}" bindchange="__e"><block wx:for="{{newsitems}}" wx:for-item="tab" wx:for-index="index1" wx:key="index1"><swiper-item><scroll-view class="list" scroll-y data-event-opts="{{[['scrolltolower',[['loadMore',[index1]]]]]}}" bindscrolltolower="__e"><block wx:for="{{team_list1}}" wx:for-item="item" wx:for-index="index"><block wx:if="{{tabIndex==1}}"><view class="item uni-flex uni-row between"><view class="uni-flex uni-row"><view class="flex_1" style="display:flex;align-items:center;margin-right:10rpx;"><image style="width:80rpx;height:80rpx;" src="{{item.avatar?item.avatar:'../../static/bar/selectuser.png'}}" mode="aspectFill"></image></view><view class="uni-flex uni-column"><view>{{item.username}}</view><view class="userType_1">{{item.jurisdiction}}</view></view></view><view class="uni-flex uni-column"><view>{{"邀请人数 "+item.count1+" 人"}}</view><view>{{"邀请人："+item.inviteusername}}</view></view></view></block></block><block wx:for="{{team_list2}}" wx:for-item="item" wx:for-index="index"><block wx:if="{{tabIndex==2}}"><view class="item uni-flex uni-row between"><view class="uni-flex uni-row"><view class="flex_1" style="display:flex;align-items:center;margin-right:10rpx;"><image style="width:80rpx;height:80rpx;" src="{{item.avatar?item.avatar:'../../static/bar/selectuser.png'}}" mode="aspectFill"></image></view><view class="uni-flex uni-column"><view>{{item.username}}</view><view class="userType_1">{{item.jurisdiction}}</view></view></view><view class="uni-flex uni-column"><view>{{"邀请人数 "+item.count1+" 人"}}</view><view>{{"邀请人："+item.inviteusername}}</view></view></view></block></block><view class="uni-tab-bar-loading">{{''+tab.loadingText+''}}</view></scroll-view></swiper-item></block></swiper></view>